
TARGETS= pro
CFLAGS= -g -c -std=gnu99 -I$(INC)
CC = gcc
BIN = bin
SRC = src
OBJ = obj
INC = include

default : $(TARGETS)

$(TARGETS) : lectura.o greedy.o timer.o main.o busquedalocal.o
	$(CC) $^ -o $@

lectura.o: lectura.c lectura.h greedy.c greedy.h timer.c timer.h busquedalocal.c busquedalocal.h
	$(CC) $(CFLAGS) $< -o $@

clean:
	rm *.o

mrproper: clean
	rm $(TARGETS)
